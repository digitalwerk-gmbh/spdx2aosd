{
  "name": "aosdconverter",
  "version": "1.0.0-beta",
  "author": "Digitalwerk GmbH",
  "description": "cli tool for converting different formats into the aosd json format. GroupSpec (AUDI, VW) spdx2.3 to aosd2.1 JSON converter written in typescript.",
  "license": "UNLICENSED",
  "main": "main.ts",
  "scripts": {
    "up": "cross-env RUN_MODE=production VERSION=up ts-node main.ts",
    "down": "cross-env RUN_MODE=production VERSION=down ts-node main.ts",
    "spdx": "cross-env RUN_MODE=production VERSION=spdx ts-node main.ts",
    "licenses": "cross-env RUN_MODE=production ts-node licenses.ts",
    "test": "cross-env RUN_MODE=test INPUT_JSON_PATH=./tests/data/input/ OUTPUT_JSON_PATH=./tests/data/output/ jest"
  },
  "repository": {
      "type": "gitlab",
      "url": "gitlab+https://gitlab.com/digitalwerk/checkoss/aosdtoaosdconverter.git"
  },
  "keywords": [
    "aosd2.1",
    "aosd2.0",
    "spdx2.3",
    "converter",
    "json",
    "groupspec",
    "scheme",
    "licenses"
  ],
  "dependencies": {
    "ajv": "^8.16.0",
    "ajv-errors": "^3.0.0",
    "axios": "^1.7.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5"
},
"devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.14.9",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"  
}
}